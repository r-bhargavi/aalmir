            <field name="prefix">6</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_asset" model="ir.sequence">
            <field name="name">Asset Internal Number</field>
            <field name="code">asset.internal.number</field>
            <field name="prefix">7</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_product_other" model="ir.sequence">
            <field name="name">Other Product Number</field>
