<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
	<!--#CH_N057 add data for sale status -->
 	<record id="sale_status1" model="sale.order.line.status">
            <field name="name">New</field>
            <field name="n_string">new</field>
            <field name="description">No operation performed on current record</field>
        </record>
	<record id="sale_status2" model="sale.order.line.status">
            <field name="name">Production-Request</field>
            <field name="n_string">production_request</field>
            <field name="description">Production Request is send to Manufacturing Department</field>
        </record>
	<record id="sale_status3" model="sale.order.line.status">
            <field name="name">Manufacturing</field>
            <field name="n_string">manufacture</field>
            <field name="description">Manufacturing order is generated by manufacturing department</field>
        </record>
	<record id="sale_status4" model="sale.order.line.status">
            <field name="name">Purchase</field>
            <field name="n_string">purchase</field>
            <field name="description">Purchase order is generated by purchase department</field>
        </record>
	<record id="sale_status5" model="sale.order.line.status">
            <field name="name">Warehouse</field>
            <field name="n_string">warehouse</field>
            <field name="description">Quantity are reserved for current order product in stock</field>
        </record>
	<record id="sale_status6" model="sale.order.line.status">
            <field name="name">Ready-To-Dispatch</field>
            <field name="n_string">r_t_dispatch</field>
            <field name="description">Sale Support person is validate the delivery order and send to logistics for dispatch</field>
        </record>
	<record id="sale_status7" model="sale.order.line.status">
            <field name="name">Dispatch</field>
            <field name="n_string">dispatch</field>
            <field name="description">Logistics has bean Full disatched the current order product</field>
        </record>
	<record id="sale_status8" model="sale.order.line.status">
            <field name="name">Partial-Dispatch</field>
            <field name="n_string">partial_dispatch</field>
            <field name="description">Logistics has bean partialy disatched the current order product</field>
        </record>
	<record id="sale_status9" model="sale.order.line.status">
            <field name="name">Partialy-Delivered</field>
            <field name="n_string">partial_delivery</field>
            <field name="description">Logistics has bean partialy delivered the current order product to customer</field>
        </record>
	<record id="sale_status10" model="sale.order.line.status">
            <field name="name">Fully-Delivered</field>
            <field name="n_string">delivered</field>
            <field name="description">Logistics has bean Full delivered the current order product to customer</field>
        </record>
	<record id="sale_status11" model="sale.order.line.status">
            <field name="name">Partial-Invoice</field>
            <field name="n_string">partial_invoice</field>
            <field name="description">Partial invoice is made for current order product</field>
        </record>
	<record id="sale_status12" model="sale.order.line.status">
            <field name="name">Full-Invoice</field>
            <field name="n_string">invoiced</field>
            <field name="description">Full invoice is made for current order product</field>
        </record>
	
	<!--record id="sale_status13" model="sale.order.line.status">
            <field name="name">Partial paid</field>
            <field name="n_string">p_paid</field>
            <field name="description">partial invoice is paid for current order product</field>
        </record-->
	<record id="sale_status14" model="sale.order.line.status">
            <field name="name">Paid</field>
            <field name="n_string">paid</field>
            <field name="description">Invoice is fully paid for current order product</field>
        </record>
        
	<record id="sale_status15" model="sale.order.line.status">
            <field name="name">Quality-Check</field>
            <field name="n_string">quality_check</field>
            <field name="description">Quantity is manufactured/Pruchased and send to quality department for quality checking</field>
        </record>
	<record id="sale_status16" model="sale.order.line.status">
            <field name="name">Date-request</field>
            <field name="n_string">date_request</field>
            <field name="description">Manufacture/purchase manager send new manufacturing/purchasing complation date</field>
        </record>
	<record id="sale_status17" model="sale.order.line.status">
            <field name="name">Delivery-Delay</field>
            <field name="n_string">delivery_delay</field>
            <field name="description">Delivery is delayed by logistics</field>
        </record>
	<record id="sale_status18" model="sale.order.line.status">
            <field name="name">Scrap-Checking</field>
            <field name="n_string">scrap_checking</field>
            <field name="description">quantity in scrap after rejected from quality checking of product</field>
        </record>
	<!--#CH_N057 end -->
	<record id="sale_status19" model="sale.order.line.status">
            <field name="name">Extra-Product</field>
            <field name="n_string">extra_product</field>
            <field name="description">Extra product is produced from manufacturing</field>
        </record>

	<record id="sale_status20" model="sale.order.line.status">
            <field name="name">Done</field>
            <field name="n_string">done</field>
        </record>
	<record id="sale_status21" model="sale.order.line.status">
            <field name="name">Cancel</field>
            <field name="n_string">cancel</field>
            <field name="description">Cancel current order product</field>
        </record>
        <record id="sale_status22" model="sale.order.line.status">
            <field name="name">Pre-Stock</field>
            <field name="n_string">pre_stock</field>
            <field name="description">quantity is with inventory but they not stored in store location</field>
        </record>
        <record id="sale_status23" model="sale.order.line.status">
            <field name="name">Force-Reserve</field>
            <field name="n_string">force_reserve</field>
            <field name="description">The Line is Force fully reserveed from Delivery Order by Sale Support.</field>
        </record>
        
	<record id="new_payment_term" model="account.payment.term">
            <field name="name">New-Payment-Term</field>
            <field name="active">True</field>
	    <field name="n_new_request">True</field>
        </record>
       <record id="milestone_payment_term" model="account.payment.term">
            <field name="name">Milestone Payment Term</field>
            <field name="active">True</field>
	    <field name="n_milestone_request">True</field>
        </record>
    </data>
</openerp>

