<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    	<!-- Film Production Sequence -->
        <record id="seq_production_request_film" model="ir.sequence">
            <field name="name">Manufacturing Request</field>
            <field name="code">film.production.request</field>
            <field name="prefix">PRF</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
     
        <!-- Injection Production Sequence -->
        <record id="seq_production_request_injection" model="ir.sequence">
            <field name="name">Manufacturing Request</field>
            <field name="code">injection.production.request</field>
            <field name="prefix">PRI</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
     
        <record id="seq_machine_maintenance" model="ir.sequence">
            <field name="name">Machine Maintenance</field>
            <field name="code">machine.maintenance</field>
            <field name="prefix">MMO</field>
            <field name="padding">5</field>
        </record>
        
        <record id="seq_batchnumber" model="ir.sequence">
            <field name="name">MRP BATCH NUMBER</field>
            <field name="code">mrp.order.batch.number</field>
            <field name="prefix">BTH</field>
            <field name="padding">5</field>
        </record>
 
        <record id="seq_stock_purchase_request" model="ir.sequence">
            <field name="name">Stock Purchase Request</field>
            <field name="code">stock.purchase.request</field>
            <field name="prefix">STPR</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_work_order_jobcard" model="ir.sequence">
            <field name="name">Work Order Job Card</field>
            <field name="code">mrp.work.order.jobcard</field>
            <field name="prefix">JOB</field>
            <field name="padding">5</field>
           
        </record>
        
        <record id="seq_user_purchase_request" model="ir.sequence">
            <field name="name">User Purchase Request</field>
            <field name="code">user.purchase.request</field>
            <field name="prefix">UPR</field>
            <field name="padding">5</field>
        </record>
        <!--record id="seq_workorder_sequence" model="ir.sequence">
            <field name="name">MRP Production Workcenter Line</field>
            <field name="code">mrp.production.workcenter.line</field>
            <field name="prefix">JC</field>
            <field name="padding">3</field>
        </record-->
        
    </data>
</openerp>
